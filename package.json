{
  "name": "actions-gh-teams-sync",
  "version": "1.0.0",
  "description": "GitHub Action to sync organization teams from a declarative YAML configuration using the GitHub API",
  "main": "dist/sync-teams.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsup",
    "start": "node dist/sync-teams.js",
    "test": "vitest run",
    "lint": "eslint scripts/**/*.ts tests/**/*.ts",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "keywords": [
    "github",
    "teams",
    "sync",
    "automation",
    "actions"
  ],
  "author": "John Ajera",
  "license": "MIT"
}
